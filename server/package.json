{
  "name": "network-api",
  "version": "0.0.1-SNAPSHOT",
  "description": "A Social Network project - partialy generated by Openapi Generator - with neo4J",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "startDevDB": "cd .. && docker-compose -f docker-compose-for-dev.yaml up -d",
    "stopDevDB": "cd .. && docker-compose -f docker-compose-for-dev.yaml down",
    "test": "env-cmd -f ./test/env.json --use-shell \"DEBUG=spec* mocha ./test/**/*.spec.js --file test/tools/startServer.js --timeout 12000 --exit\""
  },
  "keywords": [
    "openapi-generator",
    "openapi"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-openapi-validator": "^3.9.1",
    "js-yaml": "^3.3.0",
    "lodash": "^4.17.21",
    "neo4j-driver": "^4.1.2",
    "node-uuid": "^1.4.8",
    "ono": "^5.0.1",
    "openapi-sampler": "^1.0.0-beta.15",
    "swagger-ui-express": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "debug": "^4.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^7.1.1",
    "nock": "^13.1.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    }
  }
}
